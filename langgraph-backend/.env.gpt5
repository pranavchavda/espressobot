# GPT-5 Configuration via OpenRouter and OpenAI

# Primary: OpenRouter (for GPT-5 and FOSS models)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Fallback: OpenAI Direct (for GPT-5 when available)
OPENAI_API_KEY=your_openai_api_key_here

# Additional: Anthropic (for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Model Configuration (optional - defaults are set in code)
# Override default models for each tier
MODEL_ORCHESTRATOR=openai/gpt-5
MODEL_PRIMARY=openai/gpt-5-mini
MODEL_AUXILIARY=openai/gpt-5-nano
MODEL_SPECIALIZED=deepseek/deepseek-chat

# App Configuration
APP_URL=https://espressobot.com
LOG_LEVEL=INFO

# Database (existing)
DATABASE_URL=postgresql://espressobot:localdev123@localhost:5432/espressobot_dev