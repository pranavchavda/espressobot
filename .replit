

hidden = [".pythonlibs"]
modules = ["python-3.12", "web", "nodejs-22"]
run = "bash start_dev.sh"

[nix]
channel = "stable-24_05"

[deployment]
run = ["sh", "-c", "python app.py"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 5000
externalPort = 5000
exposeLocalhost = true

[[ports]]
localPort = 5173
externalPort = 80

[[ports]]
localPort = 5174
externalPort = 3000

[[ports]]
localPort = 5175
externalPort = 3001

[[ports]]
localPort = 5176
externalPort = 3002

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 11185186
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "bash start_dev.sh"
